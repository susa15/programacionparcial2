#include <iostream>

using namespace std;

int main()
{
    int m[4][4];
    int n=1;
    
    for(int i=0;i<4;i++)
    {
     for(int j=0;j<4;j++)
       m[j][i]=n++;
       
    }
    for(int i=0;i<4;i++)
    {
      cout<<endl;
      for(int j=0;j<4;j++)
         cout<<m[i][j]<<" ";
    }
    return 0;
}
